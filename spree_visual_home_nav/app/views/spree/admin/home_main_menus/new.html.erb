<%= render :partial => 'spree/admin/shared/error_messages', :locals => { :target => @home_main_menu } %>

<% content_for :page_title do %>
  <%= t('.new_home_main_menu') %>
<% end %>

<%= form_for [:admin, @home_main_menu] do |f| %>
  <fieldset data-hook="new_home_main_menu">
    <%= render :partial => 'form', :locals => { :f => f } %>
    <%= render :partial => 'spree/admin/shared/new_resource_links' %>
  </fieldset>
<% end %>
